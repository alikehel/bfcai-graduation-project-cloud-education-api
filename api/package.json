{
    "name": "cloud-education-api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start:dev": "nodemon ./src/index.ts",
        "start": "yarn build && node build/index.js",
        "build": "yarn clean && npx tsc",
        "clean": "rimraf build/",
        "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
        "lint": "eslint . --ext .ts",
        "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build && 'node ./build/index.js'",
        "swagger-autogen": "ts-node ./src/swagger/swagger.ts",
        "migrate:reset": "set NODE_ENV=dev& npx prisma db push && npx prisma generate && npx prisma migrate dev && npx prisma migrate reset",
        "seed": "set NODE_ENV=dev&  npx prisma generate && npx prisma db push && npx prisma db seed",
        "prisma:studio": "npx prisma studio"
    },
    "prisma": {
        "seed": "ts-node src/database/seed/index.ts",
        "schema": "src/database/schema.prisma"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
